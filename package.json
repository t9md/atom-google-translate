{
  "name": "google-translate",
  "main": "./lib/main",
  "version": "0.0.0",
  "description": "Translate and write result to editor in dock or split-pane",
  "keywords": [
    "translate"
  ],
  "repository": "https://github.com/t9md/atom-google-translate",
  "license": "MIT",
  "engines": {
    "atom": ">=1.7.0"
  },
  "devDependencies": {
    "prettier": "^1.10.2",
    "standard": "^10.0.3"
  },
  "scripts": {
    "test": "apm test",
    "watch": "fswatch -0 spec lib | xargs -0 -n1 -I{} apm test",
    "prettier": "prettier --write --no-semi --print-width=120 --no-bracket-spacing --trailing-comma=es5 'lib/**/*.js' 'spec/*.js'",
    "lint": "standard lib/*.js lib/**/*.js spec/*.js"
  },
  "standard": {
    "env": {
      "atomtest": true,
      "browser": true,
      "jasmine": true,
      "node": true
    },
    "globals": [
      "atom"
    ],
    "ignore": [
      "spec/fixtures/*"
    ]
  },
  "configSchema": {
    "showInStatusBar": {
      "order": 0,
      "type": "boolean",
      "default": true
    },
    "statusBarStyle": {
      "order": 1,
      "type": "string",
      "default": "icon + text",
      "enum": [
        "icon + text",
        "icon",
        "text"
      ]
    }
  }
}
